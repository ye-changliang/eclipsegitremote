                                                        id="clusterNamess" onchange="selectApar1()">
                                                    <option value="">请选择集群分类</option>
                                                </select>
                                                <input type="hidden" class="form-control" id="idsx" name="id"
                                                       placeholder="">
                                            </div>
                                        </div>
                                    </div>
                                    <div class="span4">
                                        <div class="btn-group">
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                        <div class="row-fluid">
                            <div class="span12">
                                <div class="row-fluid">
                                    <div class="span8">
                                        <div class="form-group">
                                            <label for="userName" class="col-sm-2 control-label">连接分类</label>
                                            <div class="col-sm-10" id="userName" name="userName">
                                                <select class="form-control selectpicker" name="hiveName"
                                                        id="hiveNamess">
                                                    <option value="">请选择连接</option>
                                                </select>
                                            </div>
                                        </div>
                                    </div>
                                    <div class="span4">
                                        <div class="btn-group">
                                        </div>
                                    </div>
                                </div>

                            </div>
                        </div>

                        <div class="row-fluid">
                            <div class="span12">
                                <div class="row-fluid">
                                    <div class="span8">
                                        <div class="form-group">
                                            <label for="userName" class="col-sm-2 control-label">Hive连接地址</label>
                                            <div class="col-sm-10">
                                                <input type="text" class="form-control" id="hiveAddressw"
                                                       name="hiveAddress"
                                                       placeholder="">
                                            </div>
                                        </div>
                                    </div>
                                    <div class="span4">
                                        <div class="btn-group"></div>
                                    </div>
                                </div>

                            </div>
                        </div>

                        <div class="row-fluid">
                            <div class="span12">
                                <div class="row-fluid">
                                    <div class="span8">
                                        <div class="form-group">
                                            <label for="userName" class="col-sm-2 control-label">连接数：</label>
                                            <div class="col-sm-10">
                                                <input type="text" class="form-control" id="totalNums" name="totalNum"/>
                                            </div>
                                        </div>
                                    </div>
                                    <div class="span4">
                                        <div class="btn-group"></div>
                                    </div>
                                </div>

                            </div>
                        </div>
                        <div class="row-fluid">
                            <div class="span12">
                                <div class="row-fluid">
                                    <div class="span8">
                                        <div class="form-group">
                                            <label for="userName" class="col-sm-2 control-label">租户名称：</label>
                                            <div class="col-sm-10">
                                                <select class="form-control selectpicker" name="userName"
                                                        id="userNamess" onchange="showPasswords()">
                                                    <option value="">请选择租户</option>
                                                </select>
                                            </div>
                                        </div>
                                    </div>
                                    <div class="span4">
                                        <div class="btn-group"></div>
                                    </div>
                                </div>

                            </div>
                        </div>
                        <div class="row-fluid">
                            <div class="span12">
                                <div class="row-fluid">
                                    <div class="span8">
                                        <div class="form-group">
                                            <label for="userName" class="col-sm-2 control-label">租户密码：</label>
                                            <div class="col-sm-10">
                                                <input type="text" class="form-control" id="userPasswords"
                                                       name="userPassword"/>
                                            </div>
                                        </div>
                                    </div>
                                    <div class="span4">
                                        <div class="btn-group"></div>
                                    </div>
                                </div>

                            </div>
                        </div>

                        <div class="row-fluid">
                            <div class="span12">
                                <div class="row-fluid">
                                    <div class="span8">
                                        <div class="form-group">
                                            <label for="userName" class="col-sm-2 control-label">连接数告警：</label>
                                            <div class="col-sm-10">
                                                <select class="form-control selectpicker" id="connectWarns"
                                                        name="connectWarn">
                                                    <option value="0.6">60%</option>
                                                    <option value="0.7">70%</option>
                                                    <option value="0.8">80%</option>
                                                    <option value="0.9">90%</option>
                                                </select>
                                            </div>
                                        </div>
                                    </div>
                                    <div class="span4">
                                        <div class="btn-group"></div>
                                    </div>
                                </div>

                            </div>
                        </div>

                        <div class="row-fluid">
                            <div class="span12">
                                <div class="row-fluid">
                                    <div class="span8">
                                        <div class="form-group">
                                            <label for="userName" class="col-sm-2 control-label">配置时间：</label>
                                            <div class="col-sm-10">
                                                <input type="text" class="form-control" id="setTimes" name="setTime"/>
                                            </div>
                                        </div>
                                    </div>
                                    <div class="span4">
                                        <div class="btn-group"></div>
                                    </div>
                                </div>

                            </div>
                        </div>

                    </div>
                </form>
            </div>
            <div class="modal-footer">
                <button type="button" class="btn btn-default" data-dismiss="modal">关闭</button>
                <button type="button" id="save" class="btn btn-primary">提交</button>
            </div>
        </div><!-- /.modal-content -->
    </div><!-- /.modal-dialog -->
</div>
<!-- /.modal -->
<!-- /.modal -->
<div class="modal fade" id="myDeletModal" tabindex="-1" role="dialog" aria-labelledby="myModalLabel" aria-hidden="true">
    <div class="modal-dialog" style="width: 70%;">
        <div class="modal-content">
            <div class="modal-header">
                <button type="button" class="close" data-dismiss="modal" aria-hidden="true">×</button>
                <h4 class="modal-title" id="deleteModal">删除信息</h4>
            </div>
            <div class="modal-body">
                确定删除？
                <input type="hidden" id="idsxx" name="ids">
            </div>
            <div class="modal-footer">
                <button type="button" class="btn btn-default" data-dismiss="modal">取消</button>
                <button type="button" id="delete" class="btn btn-primary">删除</button>
